{
    "name": "sitehandy/world",
    "description": "Provide countries, states, and cities relations database with Laravel 9-12 support.",
    "keywords": [ "world", "country", "city", "region" ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework" : "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3.3|^10.0|^11.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Sitehandy\\World\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Sitehandy Solutions",
            "email": "support@sitehandy.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sitehandy\\World\\WorldServiceProvider"
            ]
        }
    }
}
